{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {

        "env": {
            "type": "string",
            "defaultValue": "default"
        },

        "serviceBusName": {
            "type": "string",
            "defaultValue": "default"
        },

        "location": {
            "type": "string",
            "defaultValue": "default"
        },

        "skuName": {
            "type": "string",
            "defaultValue": "default"
        },

        "skuTier": {
            "type": "string",
            "defaultValue": "default"
        }
    },
    "functions": [],
    "variables": {},
    "resources": [
      {
        "name": "[concat(parameters('serviceBusName'), '-', parameters('env'))]",
        "type": "Microsoft.ServiceBus/namespaces",
        "apiVersion": "2021-11-01",
        "location": "[concat(parameters('location'))]",
        "sku": {
            "name": "[concat(parameters('skuName'))]",
            "tier": "[concat(parameters('skuTier'))]"
        },
        "properties": {}
      }
    ],
    "outputs": {}
}